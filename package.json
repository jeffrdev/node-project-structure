{
  "name": "directory-structure",
  "version": "1.0.0",
  "description": "A directory structure for projects with [Node](https://nodejs.org).",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "ip": "npm i express sequelize mariadb morgan dotenv",
    "ipd": "npm i sequelize-cli nodemon cross-env -D",
    "dev": "nodemon index.js",
    "test": "cross-env NODE_ENV=test jest",
    "start": "node index.js",
    "db:migrate:dev": "npx sequelize-cli db:migrate --env development",
    "db:migrate:tes": "npx sequelize-cli db:migrate --env test",
    "db:migrate:pro": "npx sequelize-cli db:migrate --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santiagor0jas/node-directory-structure.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/santiagor0jas/node-directory-structure/issues"
  },
  "homepage": "https://github.com/santiagor0jas/node-directory-structure#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mariadb": "^3.0.1",
    "morgan": "^1.10.0",
    "sequelize": "^6.23.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.1"
  }
}
